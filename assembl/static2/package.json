{
  "name": "assembl_v2",
  "version": "2.0.0",
  "description": "Assembl frontend",
  "main": "js/app/index",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "build": "rm -rf node_modules/react-i18nify/node_modules;webpack --progress --colors",
    "coverage": "jest --coverage",
    "dev": "webpack -d --watch",
    "eslint": "eslint . --ext .js --ext .jsx --cache",
    "eslint:fix": "eslint . --ext .js --ext .jsx --fix",
    "eslint:table": "eslint . --ext .js --ext .jsx --cache --format table",
    "eslint:summary": "eslint . --ext .js --ext .jsx --cache --format 'node_modules/eslint-friendly-formatter'",
    "format": "prettier-eslint --write 'js/**/*.js?' 'tests/**/*.js?'",
    "i18n:export": "node ./scripts/translations2messages.js && rip json2pot messages.json -k id -o ../locale/assembl-v2.pot && ./scripts/merge_po_files.bash",
    "i18n:import": "rip po2json '../locale/assembl-v2/*.po' -m messages.json -k id -o './translations'"
  },
  "author": "Bluenove",
  "license": "AGPL-3.0",
  "devDependencies": {
    "apollo-test-utils": "^0.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.28.5",
    "empty": "^0.10.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.52.0",
    "glob": "^7.1.2",
    "graphql-tools": "^1.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-transform-graphql": "^2.1.0",
    "node-sass": "^4.5.3",
    "prettier": "1.5.3",
    "prettier-eslint": "6.4.3",
    "prettier-eslint-cli": "4.2.3",
    "react-test-renderer": "^15.4.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "apollo-upload-client": "^5.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "draft-convert": "^2",
    "draft-js": "^0.10.1",
    "graphql": "^0.10.5",
    "graphql-anywhere": "^3.1.0",
    "history": "^3.0.0",
    "immutable": "~3.7.4",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "piwik-react-router": "assembl/piwik-react-router#skip_piwik_initialization",
    "prop-types": "^15.5.8",
    "react": "^15.4.1",
    "react-apollo": "^1.4.12",
    "react-bootstrap": "^0.31.2",
    "react-datepicker": "^0.41.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-intl-po": "^2.0.2",
    "react-redux": "^5.0.6",
    "react-redux-i18n": "^1.9.0",
    "react-router": "^3.0.0",
    "react-tweet-embed": "^1.0.8",
    "react-virtualized": "^9.9.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1",
    "searchkit": "1.0.0-7",
    "smoothscroll-polyfill": "^0.3.6",
    "url-join": "^2.0.0"
  },
  "//": "See https://facebook.github.io/jest/docs/webpack.html for more info",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "\\.(js|jsx)$": "babel-jest"
    }
  }
}
